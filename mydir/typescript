    [37m+    labels:[0m
    [37m+      component: 'single_node_cpu'[0m
    [37m+    expr: >[0m
    [37m+      max([0m
    [37m+        avg(1 - rate(node_cpu_seconds_total{mode="idle"}[1m]))[0m
