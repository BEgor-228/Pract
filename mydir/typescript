    [37m+        ([0m
    [37m+          ([0m
    [37m+            node_filesystem_size_bytes{fstype=~"ext.|xfs|nfs.?"}[0m
    [37m+            -[0m
    [37m+            node_filesystem_free_bytes{fstype=~"ext.|xfs|nfs.?"}[0m
    [37m+          )[0m
    [37m+          /[0m
    [37m+          node_filesystem_size_bytes{fstype=~"ext.|xfs|nfs.?"}[0m
    [37m+        )[0m
    [37m+      )[0m
    [37m+[0m
    [37m+  - record: gitlab_sli:gitlab_component_saturation:ratio[0m
    [37m+    labels:[0m
    [37m+      component: 'memory'[0m
